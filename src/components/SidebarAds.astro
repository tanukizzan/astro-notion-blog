---
export interface Props {
  trackingId: string
}

const { trackingId = '' } = Astro.props
---

<script
  async
  src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-3755790005820401"
  crossorigin="anonymous"></script>
<!-- monorawg_sidebar-sticky_AdSense3_728x90_as -->
<ins
  class="adsbygoogle"
  style="display:block"
  data-ad-client={trackingId}
  data-ad-slot="1919539916"
  data-ad-format="auto"
  data-full-width-responsive="true"></ins>
<script>
  ;(adsbygoogle = window.adsbygoogle || []).push({})
</script>
